<job id="example">
    <script language="JScript" src="../wsh-works.js"/>
    <script language="JScript">
        // 계산기 프로그램 실행
        var exec = System.exec("calc");
        while (exec.status() == System.PROCESS_RUNNING) {
            System.sleep(100);
        }
        println("Quit");
    </script>
</job>
